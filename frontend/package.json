{
  "name": "react-prisma-base-frontend",
  "version": "1.0.0",
  "description": "Application React Front End",
  "main": "server.js",
  "scripts": {
    "dev": "DOTENV_CONFIG_PATH=../backend node server.js",
    "build": "next build",
    "start": "next start",
    "doc": "jsdoc -c ./jsdoc.config.js",
    "test": "NODE_ENV=test jest --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "apollo-boost": "0.3.x",
    "apollo-cache": "1.2.x",
    "apollo-cache-inmemory": "1.5.x",
    "apollo-client": "2.5.x",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-styled-components": "^1.7.1",
    "bootstrap": "^4.3.1",
    "content-security-policy-builder": "^2.0.0",
    "date-fns": "^2.0.0-alpha.7",
    "dotenv-expand": "^5.1.0",
    "dotenv-safe": "^6.1.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.13.0",
    "express": "^4.17.1",
    "graphql": "^14.3.0",
    "graphql-tag": "^2.9.2",
    "json-colorizer": "^2.2.1",
    "json-stringify-safe": "^5.0.1",
    "lodash.debounce": "^4.0.8",
    "next": "^8.1.0",
    "next-runtime-dotenv": "^1.1.1",
    "next-with-apollo": "^3.4.0",
    "node-sass": "^4.12.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-adopt": "^0.6.0",
    "react-apollo": "^2.5.5",
    "react-dom": "^16.8.6",
    "react-transition-group": "^4.0.1",
    "reactstrap": "^8.0.0",
    "styled-components": "^4.2.0",
    "waait": "^1.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "casual": "^1.5.19",
    "dotenv-cli": "^2.0.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.13.0",
    "graphql-tools": "^4.0.0",
    "jest": "^24.8.0",
    "jest-transform-graphql": "^2.1.0",
    "jsdoc": "^3.6.2",
    "jsdoc-memberof-namespace": "^2.2.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".*": "babel-jest",
      "^.+\\.js?$": "babel-jest"
    }
  },
  "//": "This is our babel config, I prefer this over a .babelrc file",
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      }
    }
  }
}
